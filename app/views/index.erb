<div class="container">
  <div class="container">
  <h1>Check out a cafe near you!<h1>

  <ul>
    <% @users.each do |user| %>
      <% if user.cafe_location != nil %>
        <h1> Welcome <%=  user.name || user.username %>! </h1>
        <br>
          <ul>
            <li>About this cafe: <%= user.cafe_about %> </li>
            <% if @current_user != nil %>
              <li>Cafe location: <%= user.cafe_location %> </li>
            <% end %>
            <br>
          </ul>
        <% end %>
    <% end %>
  </ul>
</div>
